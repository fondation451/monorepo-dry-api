{
  "description": "Example of a mono repository which follows the DRY principle using type-architect",
  "devDependencies": {
    "lerna": "^4.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "name": "monorepo-dry-api",
  "private": true,
  "scripts": {
    "clean": "lerna run clean",
    "cleanAll": "lerna run cleanAll && rimraf -rf ./node_modules",
    "compile": "lerna run compile",
    "lint": "lerna run lint"
  },
  "workspaces": [
    "packages/*"
  ]
}
